<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc">
	<UploadCollectionItem 
	fileName="{poc>FileName}" 
	mimeType="{poc>Mimetype}" 
	thumbnailUrl="{parts:[{path:'poc>Mimetype'},{path:'poc>FileName'}],formatter:'.thumbnailURL'}"
	uploadedDate="{
		path:'poc>CreatedAt',
		type: 'sap.ui.model.type.Date',
        formatOptions: {
           pattern: 'yyyy/MM/dd'
        }
    }" 
	url="{path:'poc>FileName',formatter:'.attachmentURL'}" 
	visibleEdit="false"
	enableDelete="false" />
</core:FragmentDefinition>